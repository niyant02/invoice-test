{
    "name": "full-stack-engineer-outvzm",
    "version": "1.0.0",
    "description": "Your challenge is to build out the frontend and backend components of an invoicing application for Volansys Technologies's accounting department.",
    "main": "dist/index.js",
    "scripts": {
        "start": "npm run build && node dist/index.js",
        "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
        "build": "rimraf ./dist && tsc && cp .env dist/ && cd dist && mkdir uploads && cd uploads && mkdir images",
        "dev:typecheck": "tsc --noEmit --watch",
        "generate:all": "yarn migrate && yarn generate:prisma && yarn generate:nexus && yarn seed:prisma",
        "generate:nexus": "ts-node --transpile-only src/schema",
        "generate:prisma": "prisma generate",
        "seed:prisma": "prisma db seed --preview-feature",
        "migrate": "prisma migrate dev --name init --preview-feature",
        "migrate:reset": "prisma migrate reset --preview-feature",
        "migrate:deploy": "prisma migrate deploy --preview-feature",
        "migrate:update": "prisma migrate dev --create-only --preview-feature",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "repository": {
        "type": "git",
        "url": "http://volansys-tech-mpcszs@git.codesubmit.io/volansys-technologies-1/full-stack-engineer-outvzm"
    },
    "author": "niyant02",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "2.18.0",
        "apollo-server": "^2.25.2",
        "apollo-server-errors": "^2.5.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-formidable": "^1.2.0",
        "graphql": "^15.5.1",
        "graphql-iso-date": "^3.6.1",
        "graphql-relay": "^0.8.0",
        "graphql-scalars": "^1.10.0",
        "graphql-subscriptions": "^1.2.1",
        "graphql-upload": "^12.0.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "nexus": "^1.1.0",
        "nexus-plugin-prisma": "^0.32.0",
        "prisma": "2.18.0",
        "slug": "^5.1.0",
        "xlsx": "^0.17.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/graphql-iso-date": "^3.4.0",
        "@types/graphql-upload": "^8.0.6",
        "@types/lodash": "^4.14.171",
        "@types/node": "^16.3.0",
        "@types/slug": "^5.0.2",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-promise": "^5.1.0",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-node": "^10.0.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    }
}
